<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- State: SOLD (Disabled) -->
    <item android:state_enabled="false" android:drawable="@drawable/bg_seat_sold" />
    
    <!-- State: SELECTED -->
    <item android:state_selected="true" android:drawable="@drawable/bg_seat_selected" />
    
    <!-- State: AVAILABLE (Default) -->
    <item android:drawable="@drawable/bg_seat_available" />
</selector>